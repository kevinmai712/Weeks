<section id="serviceArea">
    <div class="container">
        <div class="copy">
            {{copy}}
        </div>
        <div class="flexcontainer">
            <div class="list">
            <h2>Our Service Area</h2>
                <ul>
                    {% for location in repeater %}
                        <li>
                            <a href="{{location.service_area_link}}">{{location.service_area}}</a>
                        </li>
                    {% endfor %}
                </ul>
            </div>

        </div>
    </div>
  <div class="gmap-click">
  <iframe src="https://www.google.com/maps/d/embed?mid=1msghPjxStv9t290TtpebYGb7esSG70Kd"  class="gmap-click" width="100%" height="600" frameborder="0" style="border:0; pointer-events:none;" allowfullscreen=""></iframe>
  </div>
</section>